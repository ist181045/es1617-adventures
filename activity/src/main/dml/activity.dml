package pt.ulisboa.tecnico.softeng.activity.domain;

class Activity {
    String name;
    String code;
    int minAge;
    int maxAge;
    int capacity;
}

class Booking {
    String reference;
    String cancellation;
    LocalDate cancellationDate;
}

class ActivityOffer {
	LocalDate begin;
	LocalDate end;
	int capacity;
}

relation ActivityOfferhasBookings {
	ActivityOffer playsRole activityoffer {
		multiplicity 1..1;
	}
	Booking playsRole booking {
		multiplicity 0..*;
	}
}
