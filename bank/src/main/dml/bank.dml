package pt.ulisboa.tecnico.softeng.bank.domain;

class Bank {
	String name;
	String code;
}

class Account {
    String IBAN;
    int balance;
}

enum Operation.Type;

class Operation {
    String reference;
    int value;
    DateTime time;
    Operation.Type type;
}

relation DomainRootHasBankApplication {
    .pt.ist.fenixframework.DomainRoot playsRole root {
        multiplicity 1..1;
    }

    Bank playsRole bank {
        multiplicity 0..*;
    }
}

relation BankhasAccount {
    Bank playsRole bank {
        multiplicity 1..1;
    }

	Account playsRole account {
		multiplicity 0..*;
    }
}

relation OperationHasAccount {
    Operation playsRole operation {
        multiplicity 0..*;
    }

    Account playsRole account {
        multiplicity 1..1;
    }
}